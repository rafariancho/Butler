@model List<Butler.Models.DailyMenu>
@{
    ViewData["Title"] = "Home Page";
}

My current week.
<div class="row">
    @foreach (var menu in Model)
    {
        <div class="col-lg-2">
            <h2>@menu.Day</h2>
            <div><a href="@Url.Action("Edit", "Dish")/@menu.Menu.Lunch.Id">@menu.Menu.Lunch.Name</a></div>
            <div><a href="@Url.Action("Edit", "Dish")/@menu.Menu.Dinner.Id">@menu.Menu.Dinner.Name</a></div>
        </div>
    }
</div>
