@model List<Butler.Models.DailyMenu>
@{
    ViewData["Title"] = "Home Page";
}

My current week.
<div class="row">
    @foreach (var menu in Model)
    {
        <div class="col-lg-2">
            <h2>@menu.Day</h2>
            <div>@menu.Menu.Lunch.Name</div>
            <div>@menu.Menu.Dinner.Name</div>
        </div>
    }
</div>
